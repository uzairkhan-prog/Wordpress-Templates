{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "gutenify/slider",
  "category": "gutenify",
  "title": "Slider",
  "version": "2",
  "attributes": {
    "blockClientId": {
      "type": "string",
      "default": ""
    },
    "blockAdvanceOptions": {
      "type": "object",
      "default": {}
    },
    "columns": {
      "type": "number",
      "default": 1
    },
    "spaceBetween": {
      "type": "number",
      "default": 10
    },
    "hasNavigation": {
      "type": "boolean",
      "default": true
    },
    "hasPagination": {
      "type": "boolean",
      "default": true
    },
    "isAutoplay": {
      "type": "boolean",
      "default": false
    }
  },
  "editorScript": [
    "file:./index.js"
  ],
  "style": [
    "file:./style-index.css",
    "gutenify-swiper"
  ],
  "viewScript": [
    "gutenify-swiper",
    "gutenify-common-scripts-slider"
  ]
}